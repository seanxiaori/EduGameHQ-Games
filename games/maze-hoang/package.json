{
  "name": "mazegame",
  "displayName": "The Maze Game",
  "version": "1.1.0",
  "description": "A simple but interesting maze game",
  "main": "index.js",
  "scripts": {
    "install-deps": "npm install",
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "webpack --mode production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "serve": "http-server -o",
    "serve:static": "http-server -o",
    "dev:python": "cd src/python && pip install -r requirements.txt && python main.py",
    "backend": "cd src/python && pip install -r requirements.txt && python main.py",
    "lint": "eslint src/**/*.js server/**/*.js",
    "lint:fix": "eslint src/**/*.js server/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,json,css,md}\""
  },
  "keywords": [
    "maze",
    "game",
    "puzzle",
    "pathfinding",
    "maze-generator",
    "javascript",
    "canvas",
    "html5",
    "browser-game",
    "multiplayer",
    "leaderboard",
    "achievements",
    "progressive-web-app",
    "pwa",
    "websocket"
  ],
  "author": "Son Nguyen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangsonww/The-Maze-Game.git"
  },
  "bugs": {
    "url": "https://github.com/hoangsonww/The-Maze-Game/issues"
  },
  "homepage": "https://hoangsonww.github.io/The-Maze-Game/",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "babel-loader": "^9.1.3",
    "cordova-android": "^12.0.1",
    "cordova-ios": "^7.0.1",
    "electron": "^29.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.33.2",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.2",
    "open": "^10.1.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "supertest": "^6.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ]
  },
  "dependencies": {
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node": "^16.20.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
    "socket.io": "^4.6.1",
    "winston": "^3.11.0"
  }
}
